{
  "name": "wayfarer-main",
  "version": "1.0.0",
  "description": "",
  "main": "bin/www",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon babel-node ./bin/www",
    "test": "mocha --reporter spec --exit --t=10000 --color=true  test/*.js --exec babel-node",
    "test:watch": "nyc mocha  --require babel-core/register --watch --reporter  spec   test/ --exec babel-node",
    "eslint": "eslint app/**/*.js test/*.js --fix --ignore-pattern \"node_modules/\"",
    "test:dev": "cross-env NODE_ENV=test mocha --reporter spec --exit --t=10000 --color=true  test/*.js --exec babel-node",
    "coveralls": "cross-env istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "nyc":"nyc mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CeoFred/wayfarer.git"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CeoFred/wayfarer/issues"
  },
  "homepage": "https://github.com/CeoFred/wayfarer#readme",
  "nyc": {
    "all":true,
    "check-coverage": true,
    
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": true
  }
}
